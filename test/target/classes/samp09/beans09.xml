<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="out" class="samp09.FileOutput">
		<property name="fileName" value="t1.txt"></property>
	</bean>
	<!-- 문자는 value이고 객체는 ref -->
	<bean class="samp09.MessageBeanImpl">
		<property name="name" value="보검"></property>
		<property name="greet" value="Hello"></property>
		<property name="out" ref="out"></property>
	</bean>
</beans>
